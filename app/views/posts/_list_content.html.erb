<ul class="posts list-unstyled">
  <%= content_tag_for('li', @posts, class: 'panel', itemscope: '',itemtype: 'http://schema.org/BlogPosting') do |post| %>
      <div class="panel-heading clearfix">
        <div class="col-md-12">
          <h2 class="h6" itemprop="headline"><%= link_to post.title, post_path(post), itemprop: 'url' %></h2>
        </div>
      </div>
      <div class="panel-body">
        <p><%= post.subtitle %></p>
      </div>
  <% end %>
</ul>
